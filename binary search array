#include <iostream>
using namespace std;
int binarysearch(int arr[],int n,int key){
    int s=0;
    int e=5;
    
    int mid=s+(e-s)/2;
    for(int i=0;i<n;i++){
        if(arr[mid]==key)
           return mid;
        else if(key>arr[mid]){
            s=mid+1;
        }
        else{
            e=mid-1;
        }
    }
}

int main()
{
    int arr[]={6,8,10,11,45};
    int n=5;
    int key=45;
    int ans=binarysearch(arr,5,45);
    cout<<"index is="<<ans;

    return 0;
}
